    "postversion": "git push && git push --tags",
    "docker": "npm-run-all docker:*",
    "docker:build": "docker build -t registry.yandex.net/paskills/alice-in-business-console:$USER ./",
    "docker:push": "docker push registry.yandex.net/paskills/alice-in-business-console:$USER",
    "test": "NODE_ENV=testing jest",
    "test:watch": "NODE_ENV=testing jest --watch"
  },
