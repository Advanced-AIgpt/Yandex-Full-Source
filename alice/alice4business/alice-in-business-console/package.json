    "build:assets": "mkdir -p build && cp -r src/client/assets build/",
    "build:client": "webpack --mode production",
    "build:server": "tsc && rm -rf build/client build/test build/types",
    "clean": "rm -rf build",
    "dev": "CONFIG_ENV=local magicdev",
