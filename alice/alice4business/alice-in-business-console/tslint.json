{
  "extends": "@paskills/configs/tslint",
  "rules": {
    "max-classes-per-file": false,
    "no-namespace": false,
    "member-ordering": false,
    "no-floating-promises": true,
    "interface-name": false,
    "whitespace": [true, "check-module"]
  },
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
