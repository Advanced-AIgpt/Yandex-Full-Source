{
    "meta": {
        "name": "uniproxy",
        "maintainer": "<yakovdom@yandex-team.ru>",
        "description": "Tts_cache_loader package",
        "version": "{revision}"
    },

    "build": {
        "loader": {
            "targets": [
                "alice/uniproxy/tools/tts_memcache_loader"
            ]
        }
    },

    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "build_key": "loader",
                "path": "alice/uniproxy/tools/tts_memcache_loader"
            },
            "destination": {
                "path": "/loader"
            }
        }
    ]
}
