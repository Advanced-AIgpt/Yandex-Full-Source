{
    "build": {
        "targets": [
            "alice/analytics/new_logviewer"
        ]
    },
    "data": [
        {
            "destination": {
                "path": "/usr/local/bin/"
            },
            "source": {
                "files": [
                    "logviewer"
                ],
                "path": "alice/analytics/new_logviewer/bin",
                "type": "BUILD_OUTPUT"
            }
        },
        {
            "destination": {
                "path": "/configs/"
            },
            "source": {
                "files": [
                    "*.yaml"
                ],
                "path": "alice/analytics/new_logviewer/configs",
                "type": "ARCADIA"
            }
        },
        {
            "destination": {
                "path": "/templates/"
            },
            "source": {
                "files": [
                    "*.html"
                ],
                "path": "alice/analytics/new_logviewer/templates",
                "type": "ARCADIA"
            }
        }
    ],
    "meta": {
        "description": "new logviewer for analytics with love",
        "maintainer": "Andrey Shein <andreyshspb@yandex-team.ru>",
        "name": "logviewer",
        "version": "{revision}"
    }
}
