{
    "meta": {
      "name": "billing_fake_ott_api",
      "maintainer": "PASkills",
      "description": "Fake OTT API for billing load tests",
      "version": "{svn_revision}.{revision}"
    },
    "build": {
      "targets": [
        "alice/paskills/billing_fake_ott_api"
      ],
      "target-platforms": [
        "DEFAULT-LINUX-X86_64"
      ]
    },
    "data": [
      {
        "source": {
          "type": "RELATIVE",
          "path": "Dockerfile"
        },
        "destination": {
          "path": "/Dockerfile"
        }
      },
      {
        "source": {
          "type": "RELATIVE",
          "path": "config.json"
        },
        "destination": {
          "path": "/config.json"
        }
      },
      {
        "source": {
          "type": "BUILD_OUTPUT",
          "path": "alice/paskills/billing_fake_ott_api/billing_fake_ott_api"
        },
        "destination": {
          "path": "/"
        }
      }
    ]
  }
