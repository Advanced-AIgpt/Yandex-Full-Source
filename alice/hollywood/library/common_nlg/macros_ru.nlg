{% macro get_alice_name() -%}
  {%- if context.is_yandex_spotter -%}
    Яндекс
  {%- else -%}
    Алиса
  {%- endif -%}
{%- endmacro %}

{% macro day_part_title(day_part) -%}
  {% if day_part == "morning" %}
    утро
  {% elif day_part == "day" %}
    день
  {% elif day_part == "evening" %}
    вечер
  {% elif day_part == "night" %}
    ночь
  {% endif %}
{%- endmacro %}
