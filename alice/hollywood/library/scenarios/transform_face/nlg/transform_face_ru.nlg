{% card render_gallery %}
    {
        "states": [
            {
                "state_id": 1,
                "blocks": [
                    {
                        "size": "xs",
                        "type": "div-separator-block"
                    },
                    {
                        "type": "div-gallery-block",
                        "items": [
                            {% call(suggest_number, suggest) json_enumerate_items(context.suggests) %}
                                {
                                    "width": {
                                        "type": "numeric",
                                        "value": 64,
                                        "unit": "dp"
                                    },
                                    "height": {
                                        "type": "numeric",
                                        "value": 80,
                                        "unit": "dp"
                                    },
                                    "frame": {
                                        "style": "only_round_corners"
                                    },
                                    "type": "div-container-block",
                                    "alignment_horizontal": "center",
                                    "alignment_vertical": "center",
                                    "action": {
                                        "url": "@@mm_deeplink\u0023{{ suggest.action }}",
                                        "log_id": "{{ suggest.action }}"
                                    },
                                    "children": [
                                        {
                                            "type": "div-container-block",
                                            "alignment_horizontal": "center",
                                            "alignment_vertical": "center",
                                            "children": [
                                                {
                                                    "type": "div-image-block",
                                                    "image": {
                                                        "image_url": "{{ suggest.url }}",
                                                        "type":      "div-image-element",
                                                        "ratio":     0.8235
                                                    }
                                                }
                                            ],
                                            "width": {
                                                "type": "numeric",
                                                "value": 56,
                                                "unit": "dp"
                                            },
                                            "height": {
                                                "type": "numeric",
                                                "value": 68,
                                                "unit": "dp"
                                            }
                                        }
                                    ]
                                }
                            {% endcall %}
                        ]
                    }
                ]
            }
        ]
    }
{% endcard %}
