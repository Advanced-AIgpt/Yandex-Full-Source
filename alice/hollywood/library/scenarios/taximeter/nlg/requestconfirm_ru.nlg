{% phrase ok_yes %}
  {% voice %}
    {% chooseline %}
      Принято
      Приняла
      Хорошо
      Отлично
    {% endchooseline %}
  {% endvoice %}
  {% text  %}
    Заказ принят
  {% endtext  %}
{% endphrase %}

{% phrase ok_no %}
  {% voice %}
    {% chooseline %}
      Ладно, не берём
      Окей, пропускаем
    {% endchooseline %}
  {% endvoice %}
  {% text  %}
    Заказ пропущен
  {% endtext  %}
{% endphrase %}

{% phrase not_ok %}
  {% voice %}
    {% chooseline %}
      Не поняла, нажмите кнопку
      Непонятно, нажмите кнопку
    {% endchooseline %}
  {% endvoice %}
  {% text  %}
    Что-то пошло не так  
  {% endtext  %}
{% endphrase %}
