{
  "meta": {
    "name": "divkt-renderer",
    "maintainer": "smarttv",
    "description": "Alice DivKit-Renderer on Kotlin",
    "version": "{branch}.{svn_revision}.{revision}"
  },
  "build": {
    "targets": [
      "alice/divkt_renderer"
    ],
    "target-platforms": [
      "DEFAULT-LINUX-X86_64"
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "{package_root}/jdk"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "misc",
        "files": [
          "*"
        ]
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "{package_root}/divkt-renderer"
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}
