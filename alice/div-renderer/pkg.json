{
  "meta": {
    "name": "div-renderer",
    "maintainer": "Alice Smart Display",
    "description": "Package to build alice-div-renderer",
    "version": "{branch}.{svn_revision}.{revision}.{sandbox_task_id}"
  },
  "data": [
    {
      "source": {
        "type": "RELATIVE",
        "path": "Dockerfile"
      },
      "destination": {
        "path": "/Dockerfile"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": ".npmrc"
      },
      "destination": {
        "path": "/.npmrc"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "babel.config.js"
      },
      "destination": {
        "path": "/babel.config.js"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "package.json"
      },
      "destination": {
        "path": "/package.json"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "package-lock.json"
      },
      "destination": {
        "path": "/package-lock.json"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "tsconfig.json"
      },
      "destination": {
        "path": "/tsconfig.json"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "src",
        "files": [
          "*"
        ]
      },
      "destination": {
        "path": "/src/"
      }
    },
    {
      "source": {
        "type": "RELATIVE",
        "path": "eslint-plugin-divkit",
        "files": [
          "*"
        ]
      },
      "destination": {
        "path": "/eslint-plugin-divkit/"
      }
    }
  ]
}
