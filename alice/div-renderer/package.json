{
    "name": "@yandex-int/div-renderer",
    "version": "0.182.0",
    "private": true,
    "autobumpversion": true,
    "owners": [
        "https://abc.yandex-team.ru/services/smart_display/"
    ],
    "scripts": {
        "ci:build": "npm run build",
        "gen:project": "npm ci && npm run proto",
        "ci:unit": "tsc --noEmit && npm run test",
        "ci:deploy:master": "export TAG=$(./scripts/version.js) && ./scripts/docker.sh && ./scripts/deploy.sh prestable",
        "build": "tsc",
        "start": "node dist/server",
        "dev:backend": "tsc -w & npm run dev:start",
        "dev:start": "nodemon --inspect dist/server",
        "dev:showcase": "cp -r src/dev/samples/* dist/dev/samples && tsc -w & nodemon dist/dev",
        "dev:ammogen": "npm run build && (PORT=10001 npm start & node ./dist/ammo-gen.js)",
        "proto": "./scripts/proto.sh",
        "test": "jest --coverage=false",
        "test:update": "npm run test -- -u",
        "init-autoupdate": "./scripts/autoupdate/generate_test_file.sh",
        "run-autoupdate": "(npm run build -- -w) & (nodemon --exec \"node dist/autoupdate.js &&  ../../smart_devices/tools/centaur_test/centaur_test execute --name show_view --payload test.json\")",
        "lint": "./node_modules/.bin/eslint --ext .jsx,.js,.ts,.tsx $ESLINT_OPTS ."
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.17.12",
        "@types/crypto-js": "4.1.1",
        "@types/express": "4.17.11",
        "@types/jest": "27.5.1",
        "@types/lodash": "4.14.169",
        "@types/uuid": "8.3.3",
        "babel-jest": "^28.1.0",
        "eslint": "8.16.0",
        "eslint-plugin-divkit": "file:eslint-plugin-divkit",
        "jest": "27.5.1",
        "nodemon": "2.0.15",
        "typescript": "4.6.2"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "@yandex-int/apphost-lib": "^0.12.1",
        "@yandex-int/i18n": "0.13.0",
        "@yandex-int/node-metrics": "0.3.3",
        "@yandex-int/yandex-logger": "8.6.5",
        "ajv": "8.11.0",
        "commander": "9.3.0",
        "crypto-js": "4.1.1",
        "date-fns": "^2.28.0",
        "date-fns-tz": "1.3.4",
        "divcard2": "^2.1.14",
        "dotenv": "16.0.1",
        "eslint-plugin-jest": "26.1.1",
        "express": "4.18.1",
        "lodash": "4.17.21",
        "pb-util": "1.0.1",
        "protobufjs": "6.11.2",
        "ts-essentials": "9.1.2",
        "ts-node": "10.8.1",
        "ts-proto": "1.110.0"
    }
}
