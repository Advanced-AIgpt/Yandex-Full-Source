{
    "build": {
        "targets": [
            "alice/amanda/cmd/amanda"
        ]
    },
    "data": [
        {
            "destination": {
                "path": "/bin/"
            },
            "source": {
                "files": [
                    "amanda"
                ],
                "path": "alice/amanda/cmd/amanda",
                "type": "BUILD_OUTPUT"
            }
        },
        {
            "destination": {
                "path": "/configs/"
            },
            "source": {
                "files": [
                    "*.yaml"
                ],
                "path": "alice/amanda/configs",
                "type": "ARCADIA"
            }
        }
    ],
    "meta": {
        "description": "Amanda",
        "maintainer": "Rustem Alkapov <alkapov@yandex-team.ru>",
        "name": "amanda-package",
        "version": "{revision}.{branch}.{sandbox_task_id}"
    }
}
