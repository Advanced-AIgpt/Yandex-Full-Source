{
    "build": {
        "targets": [
            "search/begemot/data/Megamind",
            "tools/wizard_yt/begemot_reducer/mapper/megamind"
        ]
    },
    "data": [
        {
            "destination": {
                "path": "/shard/"
            },
            "source": {
                "path": "search/begemot/data/Megamind",
                "type": "BUILD_OUTPUT"
            }
        },
        {
            "destination": {
                "path": "/binary/"
            },
            "source": {
                "files": [
                    "megamind"
                ],
                "path": "tools/wizard_yt/begemot_reducer/mapper/megamind",
                "type": "BUILD_OUTPUT"
            }
        }
    ],
    "meta": {
        "description": "Megamind Begemot mapper",
        "maintainer": "Fedor Solovev <the0@yandex-team.ru>",
        "name": "megamind_begemot_mapper",
        "version": "{revision}.{branch}.{sandbox_task_id}"
    }
}
