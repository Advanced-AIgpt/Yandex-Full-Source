{
  "meta": {
    "description": "Gamma server package",
    "version": "{revision}.{branch}.{sandbox_task_id}",
    "name": "gamma-server-package",
    "maintainer": "Timofey Shunin <galecore@yandex-team.ru>"
  },
  "build": {
    "targets": [
      "alice/gamma/server"
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "alice/gamma/server/gamma-server"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/server/testing.yaml"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/server/production.yaml"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/server/nanny/run-gamma.sh"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "SANDBOX_RESOURCE",
        "id": 936020616,
        "path": "pack/bin/push-client"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/server/nanny/push-client/push-client-config.yaml"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/nanny/nanny-json-pipe.py"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/nanny/run-logrotate.sh"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/server/nanny/logrotate/logrotate.config"
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}