{
  "meta": {
    "description": "Gamma song search skill package",
    "version": "{revision}.{branch}.{sandbox_task_id}",
    "name": "gamma-song-search-package",
    "maintainer": "Timofey Shunin <galecore@yandex-team.ru>"
  },
  "build": {
    "targets": [
      "alice/gamma/skills/song_search"
    ]
  },
  "data": [
    {
      "source": {
        "type": "BUILD_OUTPUT",
        "path": "alice/gamma/skills/song_search/song_search"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/skills/song_search/run-skill.sh"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "SANDBOX_RESOURCE",
        "id": 936020616,
        "path": "pack/bin/push-client"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/skills/common/push-client/push-client-config.yaml"
      },
      "destination": {
        "path": "/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/nanny/nanny-json-pipe.py"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/nanny/run-logrotate.sh"
      },
      "destination": {
        "path": "/",
        "attributes": {
          "mode": {
            "value": "+x"
          }
        }
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "alice/gamma/config/skills/common/logrotate/logrotate.config"
      },
      "destination": {
        "path": "/"
      }
    }
  ]
}