              - root:
                  extends: common.abstract_object
                  gramm: [sing]
                  lex: [езда,перемещение]
                  gov:
                    on_the:
                      orientation: right
                on_the: transport
              # вождение скутера
              - root:
                  extends: common.abstract_object
                  gramm: [sing]
                  lex: [вождение,езда]
                  gov:
                    inside:
                      orientation: right
                inside: transport
          - onto: fall-asleep
            lex: [сон,успокоение,покой,расслабление,расслабон,засыпание]
          - onto: work-background
            lex: [работа]
