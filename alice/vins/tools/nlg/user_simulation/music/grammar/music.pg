# music verbs

music_action:
  extends: common.abstract_action

launch_imperf:
  extends: music_action
  lex:
    - запускать
    - играть
    - проигрывать
    - ставить
    - врубать
    - включать
    - воспроизводить

sing_imperf:
  extends: music_action
  lex: [петь]

launch_perf:
  extends: music_action
  lex:
    - запустить
    - сыграть
    - проиграть
    - плей
    - play
    - поставить
    - врубить
    - включить
    - воспроизвести

sing_perf:
  extends: music_action
  lex: [спой]  

listen_perf:
  extends: music_action
  lex:
    - услышать
    - прослушать
    - послушать

listen_imperf:
  extends: music_action
  lex:
    - слушать
    - прослушивать
    - слышать

all_autoplay_launch_verbs:
  tag: action_request
  content:
    - launch_perf
    - launch_imperf

all_autoplay_launch_verbs_no_tag:
  - launch_perf
  - launch_imperf

all_autoplay_listen_verbs:
  tag: action_request
  content:
    - listen_perf
    - listen_imperf

all_sing_verbs:
  tag: action_request
  content:
    - sing_perf
    - sing_imperf

all_sing_verbs_no_tag:
  - sing_perf
  - sing_imperf

all_autoplay_verbs:
  - all_autoplay_launch_verbs
  - all_autoplay_listen_verbs

non_autoplay_verbs:
  extends: music_action
  lex:
    - найти
    - найти
    - найти
    - открыть
    - открыть
    - открыть
    - открыть
    - дать

all_launch_verbs:
  - all_autoplay_launch_verbs
  - non_autoplay_verbs

all_launch_verbs_no_tag:
  - all_autoplay_launch_verbs_no_tag
  - non_autoplay_verbs

all_music_verbs:
  - all_autoplay_verbs
  - non_autoplay_verbs
  - extends: music_action
    content:
      - root:
          root:
            extends: common.abstract_action
            lex: [скачать, закачать]
          conj_right: all_autoplay_listen_verbs
      - lex: [скачать, закачать]
  - extends: music_action
    root:
      - all_autoplay_verbs
      - non_autoplay_verbs
    conj_right:
      - lex: [скачать, закачать]

ability:
  lex: ['можешь','не могла бы ты','ты можешь','ты не могла бы','сможешь','можешь ли ты']

do_you_mind:
  root:
    tag: action_request
    content: launch_perf
  proclitic_oblig: ability

do_you_mind_sing:
  root:
    tag: action_request
    content: sing_perf
  proclitic_oblig: ability

do_you_mind_no_tag:
  root: launch_perf
  proclitic_oblig: ability

# music objects

abstract_album_collection:
  onto:
    type: collection
    proto:
      type: album
  content:
    root:
      reference: proto
      root: 
        tag: album
        content: album_keyword
      genetive: 
        reference: artist
        content: artist_mention
      attr: novelty_attr

abstract_music_keyword:
  extends: common.abstract_object
  tag: music_keyword
  content:
    - gramm: [sing]
      lex:
        - мьюзик
        - музыка
        - музон
        - музяка
        - музло
        - песня
        - трек
        - трэк
        - песенка
    - gramm: [plur]
      lex: [песня, песенка, трек, трэк]


abstract_song_collection:
  onto:
    type: collection
    proto:
      type: track
      need_similar: exact
  content:
    root:
      reference: proto
      content:
        - root:
            extends: common.relative_base
            content:
              - abstract_music_keyword
              - genre
              - extends: common.abstract_object
                root:
                  extends: common.abstract_object
                  gramm: [plur]
                  lex: [хит]
                genetive_oblig: genre
              - extends: [common.has_children, common.abstract_object]
                content:
                  - common.what_word
                  - gramm: [sing,neut]
              - extends: common.abstract_object
                root:
                  reference: novelty
                  tag: novelty
                  content:
                    - gramm: [plur]
                      lex: [новинка]
                    - lex: [свежак,свежачок,новинка]
                    - root:
                        extends: common.abstract_object
                        gramm: [neut,sing]
                      attr_uniq_left:
                        lex: [новый,новенький,свежий,свеженький,современный,недавний,последний]
                    - root:
                        extends: common.abstract_object
                        gramm: [neut,sing]
                      any_uniq_indecl:
                        lex: [поновее,посвежее,посовременней]
          infinitive_indecl:
            root:
              extends: common.abstract_entity
              content:
                - mood_infinitive
                - activity_infinitive
            proclitic:
              lex: [чтобы,для того чтобы,чтоб,для того чтоб,затем чтобы,затем чтоб,с целью]
          attr_uniq_left_opt:
            - common.some_word
            - lex: [разный,любой,приятный,платный,хороший,хорошенький,нормальный,классный,прикольный,зачетный,клевый,офигенный]
          purpose:
            - activity_purpose
            - mood_purpose
            - genre_purpose
            - root:
                extends: common.abstract_named_object
                gramm: [sing]
                lex: [прослушивание,воспроизведение,проигрывание,игра,звучание]
                gov:
                  inside_oblig:
                    orientation: right
              inside_oblig: activity_area
          attr:
            - activity_attr
            - epoch_attr
            - language_attr
            - mood_attr
            - genre_attr
            - personal_attr
            - personal_attr
            - personal_attr
            - personal_attr
            - novelty_attr
            - instrumental_attr
          on_the: language_language
          with:
            - language_words
            - vocal_noun
          from:
            reference: album
            content: album_mention
          any_indecl: epoch_indecl
          inside_acc:
            gramm: [sing]
            content: activity_area
          under:
            root:
              extends: common.abstract_object
              gramm: [sing]
              lex: [настроение]
            attr_uniq_left: mood_attr
          inside:
            - root:
                extends: common.abstract_named_object
                gramm: [sing]
                lex: [жанр, стиль]
              name: 
                - genre
                - root: 
                    extends: common.abstract_object
                    content:
                      - gramm: [sing]
                        lex: [музыка,песня]
                      - gramm: [plur]
                        lex: [песня]
                  attr_uniq_left: genre_attr
            - root:
                extends: common.abstract_object
                gramm: [sing]
                lex: [исполнение]
              genetive_uniq: 
                - reference: artist
                  content: artist_mention
            - reference: album
              content: album_mention
            - reference: playlist
              content: playlist_mention
          indecl_genet: epoch_genet
          genetive:
            - genre_area
            - reference: artist
              content: artist_mention
            - root:
                extends: common.abstract_object
                gramm: [sing]
                lex: [авторство,сочинение]
              genetive_uniq: 
                - reference: artist
                  content: artist_mention
          near:
            reference: artist
            content: artist_mention
          within:
            - genre_by_source
            - reference: playlist
              content: playlist_mention
            - reference: album
              content: album_mention
          for:
            onto:
              genre: forchildren
            root:
              reference: genre
              tag: genre
              content:
                - root:
                    extends: common.abstract_object
                    lex: [ребенок,малыш,малышка,сын,дочь]
                  attr_uniq_left_opt:
                    gramm: [adj]
                    lex: [маленький,любимый]
                - root:
                    extends: common.abstract_object
                    gramm: [noun]
                  attr_uniq_left:
                    gramm: [adj]
                    lex: [маленький]
          relative_inside:
              root:
                gramm: [nom]
                content: common.relative_word
              relative_clause:
                - language_sing_clause
                - vocal_sing_clause
          relative_no_agree:
            - root: common.where_word
              relative_clause:
                - language_sing_clause
                - vocal_sing_clause
            - language_short
            - vocal_short
          relative:
            - reference: personality
              content:
                - root:
                    gramm: [nom]
                    content: common.relative_word
                  relative_clause_agree: like_passive_clause_me
                - root:
                    gramm: [acc]
                    content: common.relative_word
                  relative_clause: like_active_clause_me
            - root:
                gramm: [acc]
                content: common.relative_word
              relative_clause_agree: vocal_sing_clause_agree
          relative_clause_agree_opt:
            reference: personality
            content: like_passive_clause_me
          relative_clause_opt:
            reference: personality
            content: like_active_clause_me
        - root: common.relative_pro
          relative_clause_agree:
            reference: personality
            content: like_passive_clause_me
        - root: common.relative_pro
          relative_clause:
            reference: personality
            content: like_active_clause_me

# персональность

like_passive_clause_me:
  root: common.finite_clause
  action:
    root:
      extends: common.abstract_action
      gramm: ['3',fin]
      tag: personality
      lex: [нравиться]
    dative_object:
      lex: [я]  

like_active_clause_me:
  root: common.finite_clause
  action:
    root:
      extends: common.abstract_action
      gramm: ['1',act,sing,fin]
      tag: personality
      lex: [любить,обожать]
    subject:
      lex: [я]

personal_attr:
  onto:
    personality: is_personal
  root:
    tag: personality
    reference: personality
    content:
      - root:
          extends: common.abstract_attribute
          gramm: [det]
          lex: [мой]    
        conj_attr_opt:
          root:
            extends: common.abstract_attribute
            gramm: [adj]
            lex: [любимый]
          conj_attr_opt:
            lex: [самый]
      - root:
          extends: common.abstract_attribute
          gramm: [adj]
          lex: [любимый]
        conj_attr_opt:
          lex: [самый]

# новизна

novelty_attr:
  onto:
    novelty: {}
  root:
    tag: novelty
    reference: novelty
    content:
    - root:
        extends: common.abstract_attribute
      conj_attr:
        lex: [новый,новенький,свежий,свеженький,современный,недавний,последний]
      conj_attr_opt:
        lex: [самый]
    - root:
        extends: common.abstract_entity
      any_uniq_indecl:
        lex: [посвежее,поновее,посовременнее]

# языки

language_attr_strict:
  onto:
    language: {}
  root:
    tag: language
    reference: language
    content:
      - onto: russian
        lex: [русский]  
      - onto: 'not-russian'
        lex: [иностранный,иностранный,зарубежный,нерусский,английский]
      - onto: 'not-russian'
        root:
          extends: common.abstract_attribute
          lex: [родной,русский,славянский]
        proclitic_oblig:
          lex: [не]
      - onto: spanish
        lex: [испанский]
      - onto: french
        lex: [французский]
      - onto: german
        lex: [немецкий]
      - onto: italian
        lex: [итальянский]
      - onto: portuguese
        lex: [португальский]
      - onto: swedish
        lex: [шведский]
      - onto: netherlandish
        lex: [голландский,нидерландский]
      - onto: ukrainian
        lex: [украинский]
      - onto: romanian
        lex: [румынский]
      - onto: turkish
        lex: [турецкий]
      - onto: finnish
        lex: [финский]
      - onto: bosnian
        lex: [боснийский]
      - onto: malay
        lex: [малайский,малазийский]
      - onto: swahili
        lex: [суахили]

language_attr:
  - language_attr_strict
  - onto:
      language: {}
    root:
      tag: language
      reference: language
      content:
        - onto: russian
          lex: [родной,отечественный,русскоязычный,наш]  
        - onto: 'not-russian'
          lex: [западный,англоязычный]
        - onto: 'not-russian'
          root:
            extends: common.abstract_attribute
            lex: [русскоязычный,наш]
          proclitic_oblig:
            lex: [не]

language_short:
  onto:
    language: {}
  root:
    tag: language
    content:
    - root:
        extends: common.abstract_attribute
        gramm: [short,plur]
        reference: language
        content:
          - onto: russian
            lex: [русский]  
          - onto: 'not-russian'
            lex: [нерусский,английский]
          - onto: 'not-russian'
            root:
              extends: common.abstract_attribute
              lex: [русский,славянский]
            proclitic_oblig:
              lex: [не]
          - onto: spanish
            lex: [испанский]
          - onto: french
            lex: [французский]
          - onto: german
            lex: [немецкий]
          - onto: italian
            lex: [итальянский]
          - onto: portuguese
            lex: [португальский]
          - onto: swedish
            lex: [шведский]
          - onto: netherlandish
            lex: [голландский,нидерландский]
          - onto: ukrainian
            lex: [украинский]
          - onto: romanian
            lex: [румынский]
          - onto: turkish
            lex: [турецкий]
          - onto: finnish
            lex: [финский]
          - onto: bosnian
            lex: [боснийский]
          - onto: malay
            lex: [малайский,малазийский]
      proclitic_oblig:
        lex: [по]
    - root:
        extends: common.abstract_attribute
        gramm: [short,plur]
        reference: language
        content:
          - onto: 'not-russian'
            root:
              extends: common.abstract_attribute
              lex: [русский,славянский]
            proclitic_oblig:
              lex: [не по]

language_words:
  onto:
    language: {}
  content:
    - root:
        extends: common.abstract_object
        content:
          - lex: [текст]
          - gramm: [plur]
            lex: [слово]  
          - gramm: [sing,fem]
            lex: [лирика]
      attr_uniq_left: language_attr
    - root:
        extends: common.abstract_object
        content:
          - lex: [текст]
          - gramm: [plur]
            lex: [слово]  
          - gramm: [sing,fem]
            lex: [лирика]
      relative_no_agree: language_short

language_language:
  onto:
    language: {}
  content:
    - root:
        extends: common.abstract_object
        gramm: [sing]
        lex: [язык,мова]
      attr_uniq_left: language_attr
    - root:
        extends: common.abstract_object
        gramm: [sing,noun,masc]
      attr_uniq_left: language_attr_strict

language_sing_clause:
  onto:
    language: {}
  root: common.finite_clause
  action:
    - root:
        extends: common.abstract_action
        gramm: ['3',plur,act,pres,fin]
        lex: [петь,исполнять]
      on_the: language_language
      object_opt: language_words
      circ: language_short
    - root:
        extends: common.abstract_action
        gramm: ['3',act,pres,fin]
        lex: [петься,исполняться]
      on_the: language_language
      subject_opt_agree_number: language_words
      circ: language_short
    - root:
        extends: common.abstract_action
      subject_agree_number: language_words

# типы вокала

instrumental_attr:
  onto:
    vocal: {}
  root:
    tag: vocal
    reference: vocal
    content:
      onto: instrumental
      lex: [инструментальный]

vocal_attr:
  onto:
    vocal: {}
  root:
    tag: vocal
    reference: vocal
    content:
      - onto: male
        lex: [мужской,мужицкий]
      - onto: female 
        lex: [женский,девчачий]

vocal_person:
  onto:
    vocal: {}
  root:
    extends: common.abstract_object
    tag: vocal
    reference: vocal
    content:
      - onto: male
        lex: [мужчина,парень,мужик]
      - onto: female 
        lex: [женщина,девчонка,женщина,девчонка,девка]
  proclitic:
    lex: [только]

vocal_noun:
  onto:
    vocal: {}
  content:
    root:
      extends: common.abstract_object
      content:
        - lex: [голос]
        - gramm: [sing]
          lex: [вокал]  
    attr_uniq_left: vocal_attr

vocal_short:
  onto:
    vocal: {}
  root:
    tag: vocal
    reference: vocal
    content:
      onto: instrumental
      root:
        extends: common.abstract_object
        gramm: [gen]
        content:
          - lex: [текст]
          - gramm: [plur]
            lex: [слово]
          - gramm: [sing,fem]
            lex: [лирика]
      proclitic_oblig:
        lex: [без]

vocal_sing_clause:
  onto:
    vocal: {}
  root: common.finite_clause
  action:
    - root:
        extends: common.abstract_action
        gramm: ['3',act,pres,fin]
        lex: [петь,исполнять]
      subject_opt_agree_number: vocal_person
      circ: vocal_short
    - root:
        extends: common.abstract_action
        gramm: ['3',act,pres,sing,fin]
        lex: [петься,исполняться]
      instr_opt: vocal_person
      circ: vocal_short
    - tag: vocal
      reference: vocal
      content:
        - onto: instrumental
          root: common.abstract_action
          circ_oblig:
            extends: common.abstract_object
            gramm: [gen]
            content:
              - lex: [текст]
              - gramm: [plur]
                lex: [слово]
              - gramm: [sing,fem]
                lex: [лирика]
          proclitic_oblig:
            lex: [нет]
        - onto: instrumental
          root:
            extends: common.abstract_action
            gramm: ['3',act,pres,plur,fin]
            lex: [петь]
          proclitic_oblig:
            lex: [не]

vocal_sing_clause_agree:
  onto:
    vocal: {}
  root: common.finite_clause
  action:
    - root:
        extends: common.abstract_action
        gramm: ['3',act,pres,fin]
        lex: [петь,исполнять]
      subject_opt_agree_number: vocal_person
      circ: vocal_short
    - root:
        extends: common.abstract_action
        gramm: ['3',act,pres,fin]
        lex: [петься,исполняться]
      instr_opt: vocal_person
      circ: vocal_short

