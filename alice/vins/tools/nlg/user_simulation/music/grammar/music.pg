# эпохи

epoch_indecl:
  onto:
    epoch: {}
  root:
    tag: epoch
    reference: epoch
    content:
      - onto: the-greatest-hits
        lex: [на все времена]  
      - onto: nineties
        lex: [времен перестройки]

epoch_attr:
  onto:
    epoch: {}
  root:
    tag: epoch
    reference: epoch
    content:
      - onto: the-greatest-hits
        lex: [вечный, золотой]  
      - onto: nineties
        lex: [перестроечный]

epoch_genet:
  onto:
    epoch: {}
  root:
    extends: common.abstract_object
    content:
      - lex: [годов,годков]
      - {}
  any_uniq_indecl:
    tag: epoch
    reference: epoch
    root:
      extends: common.abstract_entity
      content:
        - onto: fifties
          lex: [50-х,пятидесятых,'50','1950','1950-х']
        - onto: sixties
          lex: [60-х,шестидесятых,'60','1960','1960-х']
        - onto: seventies
          lex: [70-х,семидесятых,'70','1970','1970-х']
        - onto: eighties
          lex: [80-х,восьмидесятых,'80','1980','1980-х']
        - onto: nineties
          lex: [90-х,девяностых,'90','1990','1990-х']
        - onto: zeroes
          lex: [нулевых]
    proclitic:
      lex: [конца, начала, середины]      

# жанры

genre:
  onto:
    genre: {}
  content:
    - genre_plur
    - genre_sing

genre_plur:
  onto:
    genre: {}
  extends: common.abstract_object
  root:
    reference: genre
    gramm: [plur]
    tag: genre
    content:
      - onto: romances
        lex: [романс]

genre_sing:
  onto:
    genre: {}
  extends: common.abstract_object
  root:
    reference: genre
    gramm: [sing]
    tag: genre
    content:
      - onto: lounge
        content:
          - lex: [лаунж,лаундж]
          - root:
              extends: common.abstract_object
              gramm: [masc]
            any_uniq_indecl: 
              lex: [lounge]
      - onto: house
        root:
          extends: common.abstract_object
          gramm: [masc]
        any_uniq_indecl: 
          lex: [хаус,house]
      - onto: pop
        content:
          - lex: [попса]
          - root:
              extends: common.abstract_object
              gramm: [masc]
            any_uniq_indecl: 
              lex: [поп]      
      - onto: tradjazz
        root:
          extends: common.abstract_object
          lex: [джаз]
        attr_uniq_left:
          lex: [традиционный]
      - onto: rusrap
        root:
          extends: common.abstract_object
          gramm: [masc]
          lex: [рэп,реп,репчик,рэпчик,рэпчина,рэпачок,репчина]
        attr_uniq_left:
          lex: [русский,отечественный,местный,наш]
      - onto: rusestrada
        root:
          extends: common.abstract_object
          lex: [эстрада]
        attr_uniq_left:
          lex: [русский]
      - onto: rusrock
