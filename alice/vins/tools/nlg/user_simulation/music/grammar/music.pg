                  lex: [спорт,бег,велосипед]
              - root:
                  extends: common.abstract_object
                  content:
                    - gramm: [sing]
                      lex: [езда]
                    - lex: [поездка,путешествие]
                  gov:
                    on_the_uniq:
                      orientation: right
                on_the_uniq: 
                  gramm: [sing]
                  lex: [велосипед]
          - onto: party
            lex: [пати]
          - onto: sex
            content:
              - lex: [секс,стриптиз]
              - root:
                  extends: common.abstract_object
                  lex: [занятие]
                  gov:
                    instr_uniq:
                      orientation: right
                instr_uniq: 
                  gramm: [sing]
                  lex: [секс,любовь,стриптиз]
      - onto: road-trip
        lex: [путешествие, пикник]
      - onto: workout
        lex: [тренировка, покатушка, тренажерка, пробежка]
