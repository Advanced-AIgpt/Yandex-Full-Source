# see http://editorconfig.org
root = true

[*]
end_of_line = lf
trim_trailing_whitespace = true
indent_size = 4
charset = utf-8
insert_final_newline = true

[*.py]
indent_style = space
indent_size = 4

[*.yaml]
indent_size = 2

[*.json]
indent_size = 2

[*.js]
indent_size = 2

[*.nlg]
indent_size = 2
