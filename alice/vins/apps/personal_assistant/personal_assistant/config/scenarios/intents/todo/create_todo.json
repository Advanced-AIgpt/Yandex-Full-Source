{
  "form": "create_todo",
  "events": [
    {
      "event": "submit",
      "handlers": [
        {
          "handler": "callback",
          "name": "universal_callback"
        }
      ]
    }
  ],
  "slots": [
    {
      "slot": "what",
      "type": "string",
      "optional": true,
      "share_tags": ["todo__what"],
      "events": [
        {
          "event": "ask",
          "handlers": [
            {
              "handler": "callback",
              "name": "nlg_callback",
              "params": {
                "phrase_id": "ask__what",
                "question": true
              }
            }
          ]
        }
      ]
    },
    {
      "slot": "date",
      "type": "date",
      "optional": true,
      "share_tags": ["todo__date"],
      "events": []
    },
    {
      "slot": "answer",
      "type": "create_todo_ans",
      "share_tags": ["create_todo_ans"],
      "optional": true
    }
  ]
}
