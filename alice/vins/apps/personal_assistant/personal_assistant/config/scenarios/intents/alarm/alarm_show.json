{
  "form": "alarm_show",
  "events": [
    {
      "event": "submit",
      "handlers": [
        {
          "handler": "callback",
          "name": "universal_callback"
        }
      ]
    }
  ],
  "slots": [
    {
      "slot": "time",
      "type": "time",
      "optional": true,
      "share_tags": ["alarm_show__time"]
    }, {
      "slot": "date",
      "types": [
        "weekdays",
        "date"
      ],
      "optional": true,
      "share_tags": ["alarm_show__date"]
    }, {
      "slot": "alarm_id",
      "types": ["num", "selection"],
      "optional": true,
      "share_tags": ["alarm_show__alarm_id"]
    }, {
      "slot": "available_alarms",
      "type": "list",
      "optional": true,
      "share_tags": ["alarm_show__available_alarms"]
    }
  ]
}
