{% macro update_intent_form_action(intent_id, answer_context, item_number) -%}
  {% set payload = dict() %}
  {%- if item_number is defined -%}
    {% do payload.update(dict(item_number=item_number)) %}
  {%- endif -%}
  {{ logged_action_url(
        [
          server_action_directive(
            'update_form',
            {
              'form_update': {
                'name': "personal_assistant.scenarios." + intent_id,
                'slots': [
                  {
                    'name': 'answer',
                    'type': 'image_result',
                    'value': answer_context,
                    'optional': True
                  }
                ]
              },
              'resubmit': True
            }
          )
        ], payload)
  }}
{%- endmacro %}

{% macro camera_open_intent_action(data, intent_id) -%}
