{% macro update_clothes_form_action(item, item_number, answer_context) -%}
  {% set payload = dict() %}
  {%- if item_number is defined -%}
    {% do payload.update(dict(item_number=item_number)) %}
  {%- endif -%}
  {{ logged_action_url(
        [
          server_action_directive(
            'update_form',
            {
              'form_update': {
                'name': 'personal_assistant.scenarios.image_what_is_this__clothes',
                'slots': [
                  {
                    'name': 'selected_clothes_crop',
                    'type': 'crop',
                    'value': {
                      'category': item.category,
                      'category_id': item.category_id,
                      'crop_orig': item.crop_orig,
                      'image': item.image,
                      'clothes_original_image': answer_context.query_url,
                    },
                    'optional': True
                  },
                  {
                    'name': 'answer',
                    'type': 'image_result',
                    'value': answer_context,
                    'optional': True
                  }
                ]
              },
              'resubmit': True
            }
          )
        ], payload)
