{
  "form": "open_current_video",
  "events": [
    {
      "event": "submit",
      "handlers": [
        {
          "handler": "callback",
          "name": "universal_callback"
        }
      ]
    }
  ],
  "slots": [
    {
      "slot": "season",
      "types": ["video_season", "num"],
      "optional": true,
      "events": []
    },
    {
      "slot": "episode",
      "types": ["video_episode", "num"],
      "optional": true,
      "events": []
    },
    {
      "slot": "action",
      "type": "video_selection_action",
      "optional": true,
      "events": []
    },
    {
      "slot": "content_provider",
      "types": [
        "video_provider",
        "string"
      ],
      "optional": true,
      "events": []
    }
  ]
}
