    assoc = dict( idx = '0b0ddb77-f2d2-4b88-ab91-c319e24e4d3c', name = 'Ассоциации на время', description = '🎲 Отгадывайте слова с помощью ассоциаций. Можно играть с Алисой или соревноваться с другим игроком. У вас будет 1 минута и вся ваша сообразительность, чтобы разгадать как можно больше слов.\n\nИгра в ассоциации развивает воображение, логику и творческие способности.\n\nПример задания: \"фрукт - обезьяна\". Ответ - \"банан\". Начав с простых ассоциаций, Алиса выбирает всё более сложные задания.\n\n🏆 7 слов, угаданных за минуту, дают право на дополнительный раунд.\n\nПопробуйте сыграть в ассоциации на время!', activation = 'Давай поиграем в «Ассоциации на время»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/c90f52c0f7b12cc0cea3/mobile-logo-round-x2' ),
