    believe = dict( idx = '28983c1c-42e4-4e5a-8629-0b9bf7eb89e1', name = 'Верю — не верю', description = 'Отличите удивительный факт от правдоподобной выдумки', activation = 'Давай поиграем в «Верю — не верю»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/81ab7b0a835eaba78738/mobile-logo-round-x2' ),
    capital = dict( idx = 'd9ff0afb-4c1b-4ee6-9c3a-ea444d59df39', name = 'Игра \"Угадай столицу\"', description = '\"Угадай столицу\" - это увлекательная игра на знание столиц государств. Игра состоит из десяти случайных вопросов. Попробуйте получить свою пятерку по географии!', activation = 'Сыграем в игру «Угадай столицу»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/222ac76f5895a4ae5ab9/mobile-logo-round-x2' ),
    captain = dict( idx = '1e183a3d-7df0-45df-85c0-355b60887224', name = '⚓Вперед, капитан! Год 1772⚓', description = 'Вы попадаете в 1772 год - эпоху мореплавания по Балтийскому морю времен Екатерины Великой. 👑\n\nВас ждут сражения с английскими пиратами, торговые и дипломатические миссии, заговоры и  русско-турецкая война. \n\nПасхалки можно найти в группе: https://vk.com/vpered1772\n\nВам придется выбирать между службой у деятельного графа Орлова, или фактического канцлера, графа Панина.\n\nУдастся ли вам найти корабль-призрак и избежать смерти?\n\nПоследствия каждого вашего выбора могут стать необратимыми, ведь ваш мичман –  будущий контр-адмирал Российской Империи, или же –  ваш возможный убийца на службе у... узнаете потом.\n\nОт некоторых заданий можно отказываться: по мере принятия решений вам будут открываться другие гавани, города и возможности сделать карьеру в 7 вариантах финала.\n\nВсе совпадения случайны, а персонажи вымышлены… хотя все прототипы, локации и события сценария могут быть найдены в Википедии. \n\nНепосредственно Патросское сражение и кампания Средиземного моря, с абордажами и дуэлями будут добавлены летом. \n\nСделано с любовью к российской истории  и при поддержке Aimylogic. \n\nНе посрамите честь Петербурга и императорского флота на Балтике! \nКапитан, прикажете поднять паруса?', activation = 'Поиграем в «Вперед, капитан!»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1535439/0f736db36ef59987c5c4/mobile-logo-round-x2' ),
    categ = dict( idx = '6c752b72-5ca4-4f23-83e4-607dfdbe798a', name = 'Игра \"Категории\"', description = 'К...? Категории! \n\nПриглашаю тебя в игру \"Категории\"! Напряги память, чтобы вспомнить все слова из предложенных категорий.\n\nЯ буду называть букву и название категории, а твоя задача назвать любое слово из данной категории, начинающееся на заданную букву. \n\nСчитаешь себя мастером категорий? Проверь свой рейтинг на сайте игры: categories.fun\n\nС нетерпением жду твоего отзыва и 5-звездочного рейтинга! :)', activation = 'Сыграем в игру «Категории»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/81deca8ab2f5da87fbb8/mobile-logo-round-x2' ),
    cave = dict( idx = 'bd09fff9-047c-4421-a57d-c2cfdd444e0b', name = 'Пещера Времени', description = 'Добро пожаловать в Пещеру Времени! \n\nВы путешественник во времени, а Пещера является порталом между прошлым и будущим. Вас ждут незабываемые приключения, знакомства с интересными персонажами, и конечно же различные локации в прошлом и будущем.\n\n\"Пещера Времени\" – это интерактивная история с более чем 20 возможными финалами. От принятых решений зависит, как будет развиваться ваша версия истории. \n\nПриятной игры!', activation = 'Запусти навык «Пещера Времени»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/682888a5b0116762a1d2/mobile-logo-round-x2' ),
    chairs = dict( idx = '5043c1f0-affb-4c7f-87fa-67f1fa52e461', name = 'Музыкальные стулья', description = 'Это веселая игра из детства. Вокруг стульев по кругу бегут участники, и когда заканчивается музыка – надо успеть сесть на стул. И одному игроку всегда не хватает места. Алиса возьмет на себя роль озорного ведущего.\nОтлично подойдет для игры с детьми или для веселой компании взрослых. Развлекайтесь! :))', activation = 'Запусти навык «Музыкальные стулья»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/122ba67c3e32727c2603/mobile-logo-round-x2' ),
    charades = dict( idx = 'acca6288-6dc6-4026-b7cf-bd978f21cc09', name = 'Страна Ребусов', description = 'Любите головоломки? В этом навыке содержатся различные интересные ребусы.\n\nАлиса будет показывать вам картинку - ребус, а вы должны будете разгадать загаданное слово. В навыке есть несколько уровней сложности от простого до сложного.', activation = 'Запусти навык «Страна Ребусов»', logo = 'http://avatars.mds.yandex.net/get-dialogs/399212/f8df7d78bf6d8e380279/mobile-logo-round-x2' ),
    citadel1 = dict( idx = 'd01b4b15-c3ce-4c0c-b1e5-a3be921bc1fa', name = 'Цитадель. Часть первая', description = 'Добро пожаловать в Цитадель! 🔑\n\nСобирайте предметы для выживания и используйте их, деритесь с врагами, умирайте, воскресайте и ни в чем себе не отказывайте!\nКаждое ваше действие будет иметь последствия.\n\nЧтобы узнать уровень здоровья и что у вас есть из снаряжения, скажите \"Рюкзак\".\n\nСделано с любовью и при поддержке Aimylogic.com ❤\nФан-группа игры: https://vk.com/citaddelle\n\nАвтор чудесных иллюстраций - Софья Мироедова.', activation = 'Поиграем в «Цитадель. Часть первая»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1535439/40f77064710432e71164/mobile-logo-round-x2' ),
    citadel2 = dict( idx = '64791e8a-7af0-4918-a5f9-168fb08558b2', name = '🔑🔑  Цитадель. Часть вторая.', description = 'Вы открыли дверь в Цитадель и превратились в кота! \n\nСобирайте предметы, побеждайте монстров с помощью караоке, кусь или хоба,  и ни в чем себе не отказывайте!\n\nУ вас конечно же 9 жизней, а вот у вашего спутника их нет.\nОстерегайтесь подкустовных выползней и прочих жутконосов.', activation = 'Запусти навык «Цитадель. Часть вторая.»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/dd9aef7ca9b2e3390c26/mobile-logo-round-x2' ),
    cities = dict( idx = '672f7477-d3f0-443d-9bd5-2487ab0b6a4c', name = 'Города', description = 'Вы мне — Самара, а я вам — Архангельск', activation = 'Сыграем в «Города»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/a22fd3e7f536e9816a56/mobile-logo-round-x2' ),
    closer = dict( idx = '7d7d8ef1-b140-4432-84cc-20c4c17c121d', name = 'Игра \"Кто ближе\"', description = 'Хей! Отлично разбираешься в вопросах с числовым ответом? Тогда тебе стоит попробовать игру \"Кто ближе\"!\n\nЦель игры \"Кто ближе\" состоит в том, чтобы первым среди всех игроков, набрать 5 победных баллов. По итогам каждого раунда можно заработать 1 победный балл, если твой ответ оказался правильным или максимально близким к правильному.\n\nДанная игра отлично подойдет для двоих человек или небольшой компании!\n\nЧтобы запустить игру, скажите: \"Алиса, давай сыграем в игру Кто ближе\".', activation = 'Поиграем в игру «Кто ближе»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/2c243be3e55bc63401bb/mobile-logo-round-x2' ),
    count = dict( idx = '1476ee72-6daa-4608-9ce6-502a3a899843', name = 'Весёлые считалочки', description = 'Рассказывает считалки для детей', activation = 'Запусти навык «Весёлые считалочки»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1535439/5b5f3ac9faf34384fad7/mobile-logo-round-x2' ),
    earlier = dict( idx = 'f3861c39-63e3-403b-80de-91fc4376a5d7', name = 'Что раньше', description = 'Что изобрели раньше: тостер или батут?', activation = 'Давай поиграем в «Что раньше»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/ce87c85fec45012d95e9/mobile-logo-round-x2' ),
    eatable = dict( idx = '03a04fca-1f6c-41a0-bd8f-c6afc6469314', name = 'Съедобное-несъедобное', description = 'Игра, в которую играли нынешние папы и мамы. Игра \"Съедобное-несъедобное\" способствует развитию ассоциативного мышления ребенка и просто поможет провести время весело.\nКроме того, в игровой форме ребенок может выучить английские слова.', activation = 'Поиграем в «Съедобное-несъедобное»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/eeb250e5cde1670b074a/mobile-logo-round-x2' ),
    exit = dict( idx = 'e8363e7c-44ca-4dd1-a17c-55973b9965a5', name = 'Квест \"Выйти из комнаты\"', description = 'В последнем обновлении была добавлена новая комната - \"Лаборатория\". К тому же, в квесте появились подсказки, которые призваны помочь вам в решении головоломок. Приятной игры!\n\nВы заперты в комнате. Только от вас зависит, сможете ли вы выбраться или нет. Выбирайте комнату, изучайте объекты и решайте загадки, чтобы найти выход. \n\nНа данный момент в игре доступно три комнаты:\n- Лаборатория – Легкий уровень сложности\n- Тюремная камера – Средний уровень сложности\n- Офис – Высокий уровень сложности\n\nВ квесте \"Выйти из комнаты\" у вас есть три базовых действия. Рекомендуем заранее просмотреть и понять смысл каждой из команд:\n- Посмотри dict( Направление ), – К примеру: Посмотри вверх, направо, прямо и т.д.\n- Исследуй dict( Объект ), – К примеру: Исследуй дверь, стол, фотографию.\n- Используй dict( Объект ), чтобы dict( Объект ), – К примеру: Используй ключ, чтобы открыть сейф; используй молоток, чтобы разбить зеркало.\n\nЕще две полезные команды:\n- Команда \"Открой Инвентарь\" напомнит вам предметы, которые вы уже исследовали ранее.\n- Команда \"Помощь\" напомнит правила и используемые команды.\n\nПредупреждение: Головоломки в квесте \"Выйти из комнаты\" имеют повышенный уровень сложности. Не отчаивайтесь, если у вас не получится решить их с первого раза. Возьмите паузу и потом снова возвращайтесь к игре.\n\nЖелаем удачи и приятной игры!\n\nЕсли вам понравилась наша игра, просим поставить 5 звезд!', activation = 'Поиграем в квест «Выйти из комнаты»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1644703/0adf387e349f86fb6d73/mobile-logo-round-x2' ),
    faster = dict( idx = '1394673e-28b6-4465-9a2a-b5b54c984c86', name = 'Быстрее, Выше, Сильнее', description = 'Проверьте олимпийский лозунг в действии', activation = 'Давай поиграем в «Быстрее, выше, сильнее»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/0252be3f4740aa128005/mobile-logo-round-x2' ),
    fear = dict( idx = '8232d1d7-bb06-452e-b91d-3dca8aa854a6', name = 'Лабиринт страха', description = 'Детская игра-квест. \nПроверь свою смелость, находчивость, удачу в лабиринте наполненном монстрами, загадками и арифметическими задачками.', activation = 'Запусти навык «Лабиринт страха»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/78eeb40a431ecec3c8f0/mobile-logo-round-x2' ),
    finish = dict( idx = '649114d8-cd8a-4be7-bafe-0965940f3347', name = 'Закончи пословицу', description = 'Я говорю начало известной пословицы - а вам нужно ее закончить.', activation = 'Запусти навык «Закончи пословицу»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1017510/88ad31e513c144b08b2d/mobile-logo-round-x2' ),
    hang = dict( idx = '032c198b-64bc-454f-a9fc-2be7a172d721', name = 'игра Весёлая Виселица', description = 'Загадывается слово из категории, которую выбирает пользователь, пользователь должен его угадать! Есть 8 прав на ошибку. Можно вводить по одной букве, а, если уже догадался, введи все слово. Возможность ввести слово \"виселица\" и увидеть состояние виселицы на данный момент.\n  Также, если надоело играть, то просто напиши \"завершить\".', activation = 'Поиграем в «Весёлую Виселицу»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1525540/5b2d5e0790bff189c781/mobile-logo-round-x2' ),
    hero = dict( idx = '7f2763da-8dd8-4a9c-bf3c-0e8191692a7e', name = 'Угадай героя сказки', description = 'Вы любите сказки? Знаете главных героев популярных сказок? Сегодня мы это проверим и выясним, кто же у нас лучший знаток сказок. Игра состоит из пяти случайных вопросов. На каждый вопрос можно взять подсказку.\n\n---\nПосле обновления общее количество вопросов увеличилось в два раза. Приятной игры!', activation = 'Сыграем в «Угадай героя сказки»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1525540/7bee6800ea0ebdc27d76/mobile-logo-round-x2' ),
    igroteka = dict( idx = 'a8485d59-e259-4a1d-b7d2-01f329fcc983', name = 'Игротека - уют и веселье для друзей', description = 'Игротека - набор игр для тебя и твоих друзей. Используй навык, чтобы вдохнуть новую жизнь в ваши посиделки. Наши игры сближают и веселят, помогают не заскучать во время дождя на улице и разнообразят домашние вечеринки. Вам не нужно будет даже вставать с дивана!\n\nИгра \"Данетки\" - это настоящий бестселлер в мире развлечений. Увлекательная игра заставит каждого пораскинуть мозгами. Почувствуй себя следопытом, разгадывая самые необычные ситуации. Кроме того, \"Данетки\" отлично развивают логику и воображение у детей.\n\nВ классическую игру \"Крокодил\" можно играть всей семьей. Дети и взрослые одинаково любят эту игру. В процессе придется искать ответы в самых дальних уголках памяти, среди тех слов, которые вы могли совсем не использовать. \"Крокодил\" особенно полезен для детей, помогая им лучше понимать смыслы слов и развивая их ассоциативное мышление.\n\nПопулярная \"Я никогда не\" сближает людей, помогает узнать друг о друге много нового и неожиданного. Исключительно веселая игра для больших и шумных компаний, где присутствуют самые разные люди. Мы рекомендуем попробовать!\n\n\"Игротека - уют и веселье для друзей\" - спутник твоей компании!', activation = 'Запусти навык «Игротека»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/3eaa759c0f5cad65f394/mobile-logo-round-x2' ),
    labyrint = dict( idx = 'db211dff-d331-42c2-ac96-3f22a1185c41', name = 'Выход из лабиринта', description = 'Игра, в которой нужно выйти из лабиринта, зная текущие координаты и наличие стен вокруг себя', activation = 'Запусти навык «Выход из лабиринта»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/96249324fb457cd14cc1/mobile-logo-round-x2' ),
    library = dict( idx = 'daa0b653-6112-4d03-a810-85075789256f', name = 'Безумная Библиотека', description = 'Расскажи историю по-своему! \n\nВеселый навык для детей. Выбирай любого персонажа и отвечай на вопросы: «Кто? Что? Почему? Что сделал? и так далее» чтобы составить собственную версию истории! \n\nРезультат может удивить и развеселить! Скорее заходи в навык «Безумная Библиотека»!', activation = 'Запусти навык «Безумная Библиотека»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/78cc06f68e03b3814913/mobile-logo-round-x2' ),
    mafia = dict( idx = '03df8623-460f-4188-9f65-8c99533242cd', name = 'Веселая мафия', description = 'Мафия-популярная во всем мире игра на логику и интуицию, в которой каждому из участников отводится роль.\nПутем проведения игроков можно выявить мирных жителей и саму мафию.\nВажную роль играет ведущий, так как именно он руководит сюжетом игры.', activation = 'Давай поиграем в «Веселую мафию»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/72edd7202675ccc00a38/mobile-logo-round-x2' ),
    math = dict( idx = '4bb18366-7de8-4617-9ccd-226db95d4a4b', name = 'Игра «Математика»', description = 'Обучение детей элементарной арифметике', activation = 'Давай поиграем в математику', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/3135b58648b5eb1793af/mobile-logo-round-x2' ),
    millionarie = dict( idx = 'cd052c55-d16b-470b-bbf1-d4e78cb4f1f8', name = 'Игра кто будет миллионером?', description = 'Все знают телевизионную игру Кто хо́чет стать миллионе́ром.\nДавайте и мы поиграем в неё.\nЯ Вам буду задавать вопросы , а вы должны дать правильно ответы на 15 вопросов. У Вас есть 3 подсказки : 50x50, помощь зала, звонок другу.\nНадеюсь мы с вами сможем стать миллионерами :)', activation = 'Давай сыграем в игру «Кто будет миллионером?»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/1c714d2093112d7c201e/mobile-logo-round-x2' ),
    milord = dict( idx = 'cd48fd87-0181-4fb1-bbc8-f95c819846bd', name = 'Да, милорд', description = 'Милорд! Король поручает Вам управление нашими землями! Удерживайте баланс между своим влиянием и богатством, иначе король непременно казнит Вас.\n\nВыбирайте лучшую стратегию и принимайте неожиданные решения в самых запутанных ситуациях, чтобы удержаться у власти как можно дольше!\n\nИгра создана на платформе aimylogic.com', activation = 'Запусти навык «Да, милорд»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/1a309e8e7d6781214dc5/mobile-logo-round-x2' ),
    moar = dict( idx = '47066bff-c5cd-4cee-97d6-b0a64acbe332', name = 'Кто больше ?', description = 'Приветствую тебя, мой дорогой друг! \nНашу планету населяют множество живых существ, и все они отличаются по своему виду и размеру.\nА хорошо ли ты знаешь мир животных? \nДавай проверим это в простой игре.\nСравни животных и выбери того, кто больше весит.', activation = 'Поиграем в «Кто больше?»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1525540/54df1c8432a549bd743f/mobile-logo-round-x2' ),
    movie = dict( idx = '8262d904-7087-4e72-8581-b1cf81b7df6e', name = 'Угадай фильм по кадру', description = 'Игра стоп кадр. \nЯ буду показывать стоп кадры из известных фильмов а вы должны угадать фильм. У меня есть три уровня сложности.\n1 - Легкий ( 1 из 2 )\n2 - Средний (1 из 4 )\n3 - Сложный ( без вариантов )', activation = 'Сыграем в «Угадай фильм по кадру»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1644703/043d59ec9cf6692496f9/mobile-logo-round-x2' ),
    number = dict( idx = 'f82da40a-0d78-460e-9021-87641aeab5e2', name = 'Угадай число', description = 'Больше или меньше? Угадаю всё от 1 до 100', activation = 'Давай поиграем в «Угадай число»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/4351f70fd07e4df0881e/mobile-logo-round-x2' ),
    numtricks = dict( idx = 'c7c8abcb-5cf3-447a-a0f6-dbaef123ee57', name = 'Числовые фокусы', description = 'Крутейшие математические фокусы с числами. Приготовьте листок бумаги и калькулятор, - это будет непросто.', activation = 'Запусти навык «Числовые фокусы»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/eae71a5863cc8200b7e5/mobile-logo-round-x2' ),
    oligarch = dict( idx = '2eea9fa8-795c-4dc1-86bb-b0b6f6eefef1', name = 'Игра \"Олигарх\"', description = 'Готов попробовать свои силы в роли олигарха? Тогда скажи: \"Алиса, давай поиграем в Игру Олигарх\". Посмотрим на что ты способен!\n\nВаше материальное состояние зависит только от ваших действий. Управляйте бизнесом, ведите дела с государственными органами, отбивайтесь от бандитов и решайте семейные проблемы, чтобы увеличить свое состояние и авторитет!\n\nСоревнуйтесь с друзьями и другими игроками за место в \"Рейтинге Олигархов\".\n\nИ помните, жизнь олигарха только на первый взгляд выглядит такой простой и спокойной! \n\n\n\n\"Рейтинг Олигархов\" можно найти на сайте нашей игры – oligarch-game.ru', activation = 'Давай поиграем в игру «Олигарх»', logo = 'http://avatars.mds.yandex.net/get-dialogs/399212/48976bc960634f52f2c8/mobile-logo-round-x2' ),
    olympus = dict( idx = '3f5de8b7-61d1-45b2-a32e-b4d6d3b8e869', name = 'Интеллектуальный Олимп', description = 'Интеллектуальная игра рассчитанная на проверку эрудиции человека. Зарабатывай очки и стань первым! Всё очень просто: правильно отвечаешь на вопрос - получаешь очки, иначе всё сгорает. Если не знаешь ответа, то воспользуйся одной из подсказок, всего у тебя четыре подсказки. Просто скажи \"стоп\', чтобы остановить игру и узнать своё место! После фразы \"стоп\" игра оканчивается и очки обнуляются. Ну что, заинтересован? Стань первым в Интеллектуальном Олимпе!', activation = 'Давай поиграем в «Интеллектуальный Олимп»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1530877/6cbcb85abdec8cc37e90/mobile-logo-round-x2' ),
    oneoftwo = dict( idx = '99d0d893-046d-4bfb-b273-d9cf8d026e18', name = 'Одно из двух', description = 'Одна из самых популярных игр на западе! Подойдет как для веселой компании, так и для одного. Я задаю неожиданные вопросы и предлагаю выбрать одно из двух. Посмотрим, насколько часто твои ответы будут соответствовать ответам большинства других игроков!', activation = 'Давай поиграем в «1 из 2»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/27e0f9c650f4fb5271d6/mobile-logo-round-x2' ),
    panda = dict( idx = 'a8fc63a3-e1ed-496f-a771-2ab85c9de8e9', name = 'Викторина Пандарина', description = 'Викторина Пандарина - проверь свои знания в игре с более чем 40000 вопросов разной сложности и тематики', activation = 'Сыграем в «Пандарина»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/5d6722529e76de723174/mobile-logo-round-x2' ),
    poem = dict( idx = '9f5e3812-af77-468f-aa54-319db2595c11', name = 'Повелитель Стихов', description = 'Новая версия, пожалуй, лучшего фентези-квеста на площадке! Теперь вы не только придумываете заклинания в стихотворной форме, но и учитесь владеть словом, как настоящий великий маг!\n\nВы — волшебник, который живёт в башне и каждый день выходит на поиски приключений. Путешествуйте по миру и выполняйте задания. Самостоятельно составляйте заклинания и решайте головоломки по словами, чтобы побеждать монстров, помогать людям, преодолевать препятствия и добывать себе магические предметы.\nНе думайте, что это так просто: от вас потребуется умение быстро слагать стихи и рифмовать слова. \n\nЧто скрывается под сенью лесных деревьев?\nКак спасти жителей деревни от неминуемой гибели ? \nКакие опасности таят зажиточные с виду городские кварталы?\n\nНа все эти вопросы вы сможете ответить используя смекалку, юмор и, конечно, поэзию. Вперед! Не стоит заставлять приключения ждать!', activation = 'Запусти навык «Повелитель Стихов»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1530877/4615f3fd407bafefda15/mobile-logo-round-x2' ),
    president = dict( idx = 'c33ea1fc-f265-41eb-a364-36b1115af626', name = 'Да, мистер Президент', description = 'Попробуйте себя в качестве президента США!\n\nВам предстоит управлять одной из крупнейших стран мира. \nПринимать множество решений, начиная от расследования внутренних интриг и заканчивая участием в международных конфликтах. \n\nИ все это для того, чтобы продержаться на посту все 4 года и избраться на второй срок! \n\nДля запуска игры скажите: \"Алиса, давай сыграем в Да, мистер Президент\".', activation = 'Сыграем в «Да, мистер Президент»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1644703/0c1c1e94d498f2d4b376/mobile-logo-round-x2' ),
    questions = dict( idx = 'd2b0e523-7575-4a87-855c-7e7066e883c1', name = 'Викторина \"Вопросы с друзьями\"', description = 'Цель викторины: правильно ответить на 7 случайных вопросов и собрать больше баллов, которые начисляются за правильные ответы. Ответы однозначные: да, либо нет. \nВ викторину: \"Вопросы с друзьями\', может играть от одного до четырех человек. В одиночной игре, Вы будете играть со случайным соперником (соперник, который уже играл в данную викторину).\n\nВопросы викторины разбиты по категориям, которую можно выбрать перед началом викторины.', activation = 'Поиграем в викторину «Вопросы с друзьями»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1027858/7e9465e49517adbfbc06/mobile-logo-round-x2' ),
    reddream = dict( idx = '841e28de-9fd8-4218-8ae4-18de6975d220', name = 'Красная мечта', description = 'Постройте красную мечту серпом и молотом. Игра заключается в ответах пользователя на вопросы по управлению страной с целью достигнуть коммунизма - игровой победы.', activation = 'Запусти навык «Красная мечта»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/d2bb875379ac13f398f1/mobile-logo-round-x2' ),
    sea = dict( idx = '32168fc9-5d95-4057-a516-2afe762953e2', name = 'Монстры на каникулах 3: море зовёт', description = 'Кто вы — неуловимый Человек-невидимка, веселый Мумий или прекрасная вампирша Мэвис? Узнайте, на какого героя фильма «Монстры на каникулах» вы похожи.', activation = 'Запусти навык «Монстры на каникулах 3: море зовёт»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/2119be7c95ef6008d643/mobile-logo-round-x2' ),
    smart = dict( idx = '5541db3f-8931-4eec-ae19-d26381da652b', name = 'Умные Сказки', description = 'Сказки с иллюстрациями и развивающими вопросами! Сначала ребёнку предлагается сказка, которую он может начать слушать, или попросить предложить ему другую. После прочтения небольшого отрывка, ребёнку задаётся вопрос: интересно ли ему, продолжать ли рассказывать дальше, или выбрать другую сказку. Затем, на протяжении всей сказки, ребёнку периодически задаются вопросы относительно того или иного сказочного эпизода, при этом его ответы не проверяются и не оцениваются. Все сказки сопровождаются музыкальными фрагментами, а некоторые - ещё и звуками животных, природы, или предметов.\n\nКогда репертуар сказок заканчивается - всё начинается сначала. Это означает, что если ребёнок отказался от прослушивания каких-то сказок ранее - он сможет послушать их позже. Кроме того, ребёнок имеет возможность сам заказать понравившуюся ему сказку, используя её название или слова близкие к названию по смыслу. Например, если ребёнку понравилась сказка \"Лиса и журавль\', то он может вызвать эту сказку фразой: \"Расскажи про журавля и лисичку\". \n\nРебёнок также может получить ответы и на ряд дополнительных, иногда неожиданных вопросов, например: \"Сколько сказок ты знаешь?\', \"Как тебя зовут?\', \"Кто твоя мама?\', \"Где ты живёшь?\" и другие. Если ребёнок не расслышит какой-либо ответ - он может переспросить: \"Повтори\', \"Не расслышал(а)\', \"Не понял(а)\". \n\nПолучить справку можно с помощью фраз: \"Справка\', \"Помощь\', \"Что ты умеешь?\', а закрываются сказки словами: \"Закрыть\', \"Выйти\', \"Хватит\', \"Достаточно\', и другими подобными. При закрытии сказок данные о навигации сохраняются, поэтому при следующем запуске - сказки откроются в нужном месте.\n\nПолитика конфиденциальности: персональные данные пользователей не отслеживаются, не собираются, и не сохраняются.\n\nПримечание: иллюстрации доступны только на устройствах с экраном.', activation = 'Запусти навык «Умные Сказки»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1530877/48b46f2ec59234d63da0/mobile-logo-round-x2' ),
    sound = dict( idx = '900892a5-0e8d-4b92-a24a-6f39e467298d', name = 'Угадай звук', description = 'Игра на угадывание звука. \nАлиса произносит звук, а ваша задача его угадать.\nКаждая игра состоит из 5 раундов.\n\nТонкость в том, что звук может быть из разных категорий, что усложняет задачу, но подсказки помогут вам её решить.', activation = 'Сыграем в «Угадай звук»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1535439/b14fb2b4ca662eaed68a/mobile-logo-round-x2' ),
    space = dict( idx = '44f3d052-2488-4a5f-b329-ed33a5206a7c', name = 'Космические приключения', description = 'Игра об одном из приключений космического путешественника. Который выполняет задание по уничтожению базы пиратов на планете Бон.\n\nОснована на одном из квестов игры \"Космические рейнджеры\". Квест был переработан и адаптирован. \n\nВ игре применена технология сопоставления ответов, поэтому можно отвечать не с точным соответствием предложенному варианту. Например, использовать лишь часть ответа или полностью перестроить фразу.\n\nСпасибо всем кому понравилась игра, будем благодарны, если поставите оценку.', activation = 'Сыграем в «Космические приключения»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1644703/3fd99f094435072e1278/mobile-logo-round-x2' ),
    speed = dict( idx = 'e152363c-4a01-42cd-8937-1b0feb4b02c5', name = 'Повтори скороговорку!', description = 'Попробуйте правильно повторить все популярные скороговорки! \n\nОтличный навык для развития дикции и памяти для детей. Всего в навыке доступно 50 уровней по 3 скороговорки в каждом.\n\nВы также можете попробовать режим \"Тренировка\', где у вас будет возможность повторяя скороговорки, потренировать произношение букв алфавита.', activation = 'Запусти навык «Повтори скороговорку!»', logo = 'http://avatars.mds.yandex.net/get-dialogs/1676983/ca0c8da5ba66f2ba4ed0/mobile-logo-round-x2' ),
    tales = dict( idx = 'bd168a52-c2eb-43be-a3df-632e207cd02b', name = 'Сказки', description = 'У меня есть много сказок. Про кого хотите послушать?', activation = 'Запусти сказку', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/164f2812c788447596cd/mobile-logo-round-x2' ),
    tamag = dict( idx = 'b961e8ab-a839-4b24-a030-dfa184a59267', name = 'Мой тамагочи', description = 'Данный навык сможет помочь вам стать настоящим любящим хозяином для своего домашнего питомца. Здесь есть возможность научиться ухаживать за самыми разными домашними животными.\nЕсли вы готовы к этому, то в первую очередь вам нужно научиться ухаживать за виртуальными существами, чтобы в реальной жизни быть готовым ко всему.\nВы будете ухаживать за животными(кормить, мыть, поить, играть) и выполнять много других обязанностей, которые присущи для каждого хозяина своего питомца.\n\nТакже у вашего питомца есть различные достижения, и с каждым днем, с вашей помощью ваш питомец становится все умнее и повышает свой уровень.\n\nДля более подробной информации по навыку можно перейти по ссылке:\nhttps://www.islandgift.ru/tamagotchi/\n\nПомимо этого, когда вы уже завели своего питомца, вы можете присоединить его к своему личному кабинету.\nДля этого вам просто необходимо:\n1. Авторизоваться на сайте https://www.islandgift.ru/registration;\n2. Зайти в навык и сказать \"Авторизоваться\". после чего перейдите на сайт.\n\nЕсли вам вдруг надоел ваш питомец, и вы хотите завести другого, тогда вы можете сделать это. Просто скажите одну из команд:\n- Изменить питомца\n- Выбрать другого питомца\n- Сбросить все\n- Начать сначала\nПри этом, для того чтобы сменить питомца, вам необходимо 500 монет.\n\nЗовите друзей, и играйте все вместе.', activation = 'Давай поиграем в «Мой тамагочи»', logo = 'http://avatars.mds.yandex.net/get-dialogs/758954/f9d0a8020c121a903596/mobile-logo-round-x2' ),
    twister = dict( idx = '32208fd9-a0d5-404c-a6d4-8f25c8571259', name = 'Игра в Твистер', description = 'Классический Твистер: Алиса крутит рулетку и называет следующий ход — действие, которое должна совершить ни в чем не повинная конечность игрока.', activation = 'Запусти навык «Игра в Твистер»', logo = 'http://avatars.mds.yandex.net/get-dialogs/998463/d98f5276bd2b0b036665/mobile-logo-round-x2' ),
