{% from "cards/common.nlg" import font_color, get_skill_click_action with context %}


{% macro render_skill_recommendation_card_multicolumn(card_name) %}
  {
    "states":[
      {
        "state_id": 1,
        "blocks":[
          {{ separator("xs") }},
          {
            "type":"div-tabs-block",
            "has_delimiter": 1,
            "items": [ {{ render_items_onb(context.data.cases[:4]) if card_name == 'onboarding' else render_items_gonb(context.data.cases[:4]) }} ]
          }
        ]
      }
    ],
    "background":[
      {
         "type":"div-solid-background",
         "color":"{{ '#ffffff' }}"
      }
    ]
  }
{% endmacro %}


{% macro render_column(col_name, col_content) %}
  {
    "title": {
      "text": "{{ col_name }}"
    },
    "content": {
      "type": "div-container-block",
      "direction": "vertical",
      "alignment_vertical":"top",
      "alignment_horizontal":"left",
      "width": {
        "type": "predefined",
        "value": "match_parent"
      },
       "height": {
        "type": "predefined",
        "value": "wrap_content"
      },
      "children": [
        {{ separator("xs") }},
        {% for case in col_content %}
          {{ render_case(case, loop.index, case.recommendation_source or 'stub', case.recommendation_type or 'stub', case.idx) }},
        {%- endfor %}
        {{ separator("xs") }}
      ]
    }
  }
{% endmacro %}


{% macro render_case(item, item_number, source, type, idx) %}
  {% set log_id = "skill_recommendation__" + source + "__" + type + "__" + idx %}
  {% set action_url = get_skill_click_action(log_id, item.activation, item_number, source) %}
  {% set title = item.activation | html_escape | capitalize_first | end_without_terminal %}
  {% set text = font_color('#7f7f7f', item.description | html_escape | capitalize_first | end_with_dot) %}
  {
    "title": "{{ title }}",
    "text": "{{ text }}",
    "type": "div-universal-block",
    "title_max_lines": 2,
    "text_max_lines": 2,
    "action": {
      "url": "{{ action_url }}",
      "log_id": "{{ log_id }}"
    },
    "side_element": {
      "element": {
        "image_url": "{{ item.logo }}",
        "ratio": 1,
        "type": "div-image-element"
      },
      "size": "s",
      "position": "left"
    }
  }
{% endmacro %}


{% macro separator(size="xxs", delimiter=False) %}
  {
    "size": "{{ size }}",
    "type": "div-separator-block"
    {% if delimiter %}
      , "has_delimiter": 1
    {% endif %}
  }
{% endmacro %}


