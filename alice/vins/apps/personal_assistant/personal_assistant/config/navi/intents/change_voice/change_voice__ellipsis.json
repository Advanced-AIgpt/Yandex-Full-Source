{
  "form": "change_voice",
  "events": [{
    "event": "submit",
    "handlers": [
      {
        "handler": "callback",
        "name": "universal_callback"
      }
    ]
  }],
  "slots": [{
    "slot": "voice",
    "type": "navi_voice",
    "optional": true,
    "events": [
      {
        "event": "ask",
        "handlers": [
          {
            "handler": "callback",
            "name": "nlg_callback",
            "params": {
              "phrase_id": "ask__voice_name"
            }
          }
        ]
      }
    ]
  }]
}
