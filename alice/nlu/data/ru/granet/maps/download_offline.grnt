form alice.maps.download_offline:
    slots:
        region:
            matching_type: overlap
            type:
                GeoAddr.Address
                sys.geo
            source: $Region

    root:
        [$Download+ $AmbiguousCurrent* $DefinitelyCurrent+ $Offline* $Maps+]
        [$Download+ $Current* $Offline+ $Maps+]
        [$DefinitelyDownload+ $Current* $Offline* $Maps+]
        [$Download+ $Current* $Offline* $DefinitelyMaps+]
        [$Download+ $Current* $Offline* $Maps+ $Region+]
        [$Common.ToWant+ $Current* $DefinitelyOffline+ $Maps+ $Region+]
        %negative
        %lemma
        [$Download (приложение|программу)? (яндекс карты)]
        [$Download* $Current* $Offline* $Maps* (пушкинская карта)]
        [$Download* $Current* $Offline* $Maps* (сим карта)]
        [$Download* $Current* $Offline* $Maps* (сим-карта)]

    $DefinitelyOffline:
        %lemma
        доступ? доступный? в? (оффлайн|офлайн|не онлайн) режиме?
        автономный доступ? режим?
        (режим|работа) (оффлайн|офлайн)
        доступ? включить? без поддержки? наличия? использования? пользования? (сети|интернета)
        [без (доступа|выхода|привязки|соединения|подключения) (в|к|с)? (инет|интернет|сеть)]
        без связи
        вне зоны? действия (сети|связи|интернета)
        (пропадет|при плохом) (связь|инет|интернет|сеть)
        будут? проблемы с? со? (связь|инет|интернет|сеть)
        безинтернетная
        без включения мобильных данных
        ((при|без|в|в случае|на случай) отсутствии|(когда|если)? не будет) (инета|интернета|сети)
        [(когда|если) (инета|интернета|сети) (нет|не будет)]

    $AmbiguousOffline:
        %lemma
        в (кэш|кеш)
        в мои? файлы
        в? роуминге
        (дальнейшее|последующее)? (использования|пользования|воспользоваться|пользоваться|смотреть|просматривать)
        (на|в) мой? мобильный? (телефон|смартфон|экран|устройство)
        (на|в) мой? мобильный
        (на|в) мою? карту памяти
        в список моих? карт
        на внешний? диск
        (на|в|во|из) внутреннюю? память телефона?
        [всегда была? доступна]
        [пользоваться ей? (в дороге|на месте|в поедке|при поездке|в дальнейшем|в любой момент|в любое время)]
        в (приложение|программу|яндекс? карты)
        в любой момент
        в любое время
        в пути
        в облако

    $Offline:
        $DefinitelyOffline
        $AmbiguousOffline

    $Region:
        $ner.GeoAddr.City
        $ner.GeoAddr.City2
        $ner.GeoAddr.Province
        $ner.GeoAddr.District
        $ner.GeoAddr.Country
        $sys.geo

