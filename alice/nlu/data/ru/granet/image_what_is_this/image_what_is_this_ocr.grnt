import: common/filler.grnt

form alice.image_what_is_this_ocr:
    root: 
        распознай и переведи этот? текст
        $Recognize этот? текст (на $Photo)?
        .* что $Particles+ .? $Written .*
        .* что .? написано .*
    filler: $nonsense

$Particles:
    %lemma
    такое
    здесь
    это
    тут
    там
    мелко
    на
    на $Photo

$Written:
    написано
    написала
    написал
    написали
    напишу
    надпись

$Photo:
    %lemma
    фотка
    фото
    фотография
    селфи
    картинка
    пикча
    рисунок
    изображение
    картина
    снимок

$Recognize:
    %lemma
    опознай
    распознай
    определи
    угадай
    отгадай
    узнай
    удентифицируй
    поясни
    объясни
    опознать
    распознать
    определить
    угадать
    отгадать
    узнать
    идентифицировать
