import: common/pronouns.grnt

$Common.Yes:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    ага
    да
    угу

$Common.No:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    не
    нет
    нету
    нетушки
    ничего

$Common.Now:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    $Common.Immediately
    $Common.PronounThis* (момент | раз | секунда | минута | час | день)+
    сегодня

$Common.Immediately:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    $Common.PronounThis+ (момент | секунда)+
    быстро
    мгновенно
    немедленно
    прямо? сейчас же?
    срочно
    скорее
    только что

$Common.Slowly:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    медленно
    медлительно
    не спеша
    неторопливо
    неторопясь
    понемногу
    потихоньку

$Common.Little:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    капельку
    маленько
    мальца
    не сильно
    незначительно
    немного
    немножко
    понемногу
    слегка
    чуток
    чуточку
    чуть+

$Common.Quick:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    быстро
    временно
    на время
    ненадолго


$Common.Very:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    жутко
    очень
    премного
    самый
    сильно
    слишком
    совсем


$Common.StrictMore:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    еще

$Common.OneMoreTime:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    и* $Common.StrictMore+ один* раз*


$Common.Another:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    %fillers off
    другой
    $Common.No+ $Common.PronounThis+


$Common.More:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    $Common.Another
    $Common.OneMoreTime
    больше


$Common.Correct:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    корректно
    правильно


$Common.Wrong:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    $Common.No+ $Common.Correct+
    некорректно
    неправильно
    ошибочно


$Common.Again:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    $Common.OneMoreTime
    заново
    опять
    сначала
    снова
    тоже

$Common.Aloud:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    в? голос
    вслух
    голосом
    громко
    громогласно

    %negative
    слушаться

$Common.Still:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    (пока|все) еще
    до сих пор

$Common.Approximately:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    примерно
    где нибудь
    около
    где то

$Common.MoreConvenient:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    легче
    лучше
    приятнее
    проще
    удобнее

$Common.Here:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    прямо? (здесь|тут|туточки|здеся)

$Common.There:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    там

$Common.Additionally:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    ещё
    тоже
    также
    дополнительно
    к тому же

$Common.Together:
    %lemma
    %enable_synonyms all
    %disable_synonyms synset
    вместе
    заодно
    совместно

$Common.BrightShort:
    %lemma
    светло
    ярко
    %force_negative
    %exact
    (ярче|поярче)
    (светлее|посветлее|светлей|посветлей)

$Common.DarkShort:
    %lemma
    бледно
    мрачно
    темно
    тускло
    %force_negative
    %exact
    (бледнее|побледнее|бледней|побледней)
    (мрачнее|помрачнее|мрачней|помрачней)
    (темнее|потемнее|темней|потемней)
    (тусклее|потусклее|тусклей|потусклей)

$Common.HumidShort:
    %lemma
    влажно
    %force_negative
    %exact
    (влажней|повлажней|влажнее|повлажнее)

$Common.DryShort:
    %lemma
    сухо
    %force_negative
    %exact
    (посуше|суше)
