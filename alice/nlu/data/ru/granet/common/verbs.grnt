import: common/adverbs.grnt


$Common.ToThank:
    $Inner:
        %lemma
        %enable_synonyms all
        %disable_synonyms synset
        thanks
        благодарить
        мерси
        спасибки
        спасибо
        спасибочки

    [$Common.Very* $Inner+]


$Common.ToWant:
    $Inner:
        %enable_synonyms all
        %disable_synonyms synset
        будем
        будешь
        буду
        %lemma
        желать
        захотеть
        мечтать
        надо
        необходимо
        нужен
        нужна
        нужно
        хотение
        хотеть

    [$Inner+ бы*]

$Common.DontWant:
    не $Common.ToWant

