import: common/adverbs.grnt
import: common/conjunctions.grnt
import: common/prepositions.grnt
import: common/pronouns.grnt
import: common/verbs.grnt

$Another:
    %lemma
    ещё
    еще
    следующая
    другую
    другая
    предыдущая
    прошлая

$Music.Filler:
    %lemma
    аудио
    будь добра
    ли
    [не? могла бы ты?]
    очень надо
    помоги
    прошу
    попробуем

$Music.HolidayRelated:
    %lemma
    праздник
    праздничное

$Music.NewYearRelated:
    %lemma
    предновогоднее|предновогодними
    новогоднее|новогодними
    для нового года

$Music.Playing:
    $Playing:
        %lemma
        %enable_synonyms all
        %disable_synonyms synset
        $Common.ToSound
        включила
        играешь
        играет
        исполняешь
        исполняется
        воспроизводишь
        воспроизводится
        поешь
        поется

    [$Common.ConjunctionThat* $Common.Now* $Common.At* $Common.PronounYou* $Common.PronounMe* $Playing+]
