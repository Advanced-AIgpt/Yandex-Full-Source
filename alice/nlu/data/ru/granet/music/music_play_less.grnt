import: common/filler.grnt

form personal_assistant.scenarios.music_play_less:
    root:
        [$Less $Kind+]

$Less:
    меньше
    поменьше
    не настолько
    все кроме
    ну не
    слишком много
    многовато

$Kind:
    $Genre
    $Language
    $Mood
    $QualifiedEpoch
    $Vocals

$Language:
    %lemma
    русского
    украинского
    американского
    английского
    испанского
    латинского
    латино
    по английски
    с англоязычной лирикой
    англоязыного
    славянского

$Vocals:
    %lemma
    с мужским $Vocal
    с женским $Vocal
    с мужицким $Vocal

$Vocal:
    %lemma
    вокалом
    лирикой
    голосом

$Genre:
    %lemma
    рок
    rock
    рок н ролл
    rock n roll
    рэп
    реп
    rap
    шансон
    попса
    поп
    pop
    метал
    metal
    техно
    techno
    транс
    trance
    хаус
    house
    джаз
    jazz
    блюз
    blues
    блатняк
    диско
    disco
    инди
    indie
    бардовских
    танцевального
    dance
    реперского
    хип хопа
    hip hop
    хардрока
    hard rock
    электроники
    electronic
    эстрады
    регги
    рэгги
    reggae
    арэнби
    аренби
    rnb
    r n b

$Mood:
    %lemma
    агрессивно
    агрессивного
    бодрого
    быстрого
    веселого
    веселее
    для веселья
    радостного
    для радости
    грустного
    депрессивного
    душевного
    легкого
    медленного
    напряженного
    печального
    для печали
    сексуального
    вселенского
    вселенской
    осеннего
    под осеннее настроение
    летнего
    под летнее настроение
    зимнего
    под зимнее настроение
    весеннего
    под весеннее настроение
    таинственного
    таинственной
    новогоднего

$QualifiedEpoch:
    [$EpochQualifier? $Epoch $EpochFiller*]

$EpochQualifier:
    начала
    конца
    середины

$EpochFiller:
    годов
    годков
    эпохи
    посвежее
    свежее
    более свежее
    более свежего

$Epoch:
    1950 х
    50 х
    пятидесятых
    1960 х
    60 х
    шестидесятых
    1970 х
    70 х
    семидесятых
    1980 х
    80 х
    восьмидесятых
    1990 х
    90 х
    девяностых
    нулевых
    2000 х
    2010 х
    десятых
    10 х

filler:
    $nonsense
    $Common.Filler
    %lemma
    включи
    следующий
    давай
    слишком
    не так
    очень
    песня
    музыка
    музон
    музона
    музло
    какой нибудь
    какой либо
    какой то
    трэк
    трек
    нравятся
    нравится
    песен
    песни
    песенки
    песенок
    любого
    чего то
    найди
