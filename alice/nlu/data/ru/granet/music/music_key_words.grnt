form alice.music.music_key_words:
    enable_synonyms: all
    disable_synonyms: synset
    root:
        .* $Content .*
        .* $Music .*

$Content:
    %lemma
    стих
    стихотворение
    проза
    поэма
    рассказ
    повесть
    роман
    книга
    аудиокнига
    аудиоэнциклопедия
    молитва
    сказка

$Music:
    %lemma
    песня
    музыка
    композиция
    трек
    исполнитель
    альбом
    плейлист
