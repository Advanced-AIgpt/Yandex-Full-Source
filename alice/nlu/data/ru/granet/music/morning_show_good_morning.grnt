import: music/morning_show_common.grnt

form personal_assistant.scenarios.morning_show_good_morning:
    root:
        [$TurnOn* $GoodMorning+]

    filler:
        $Filler

$TurnOn:
    %lemma
    с
    хочу
    начни

$Good:
    %lemma
    доброе
    хорошее

$Morning:
    %lemma
    утречко
    утро

$GoodMorning:
    [$Good+ $Morning+]
    добрутро
