import: common/commands.grnt
import: common/filler.grnt

form personal_assistant.scenarios.get_news__more:
    root:
        $Root

        %negative
        [.* $Negative]

    filler:
        $Common.Filler

$Root:
    [$Common.LeftFiller? $nonsense?] [еще? $What? $Verb? какие? $More $News?]
    [$Common.LeftFiller? $nonsense?] [какие? ($More|еще) $News]

$NotNews:
    %lemma
    воспроизведение
    музыку
    фильм
    игры
    просмотр
    играть

$Negative:
    еще раз
    не надо
    не знаю
    хватит
    повтори
    $Verb? $NotNews

$What:
    какие
    чё
    что
    что там
    что по
    что по поводу
    что про
    чего

$Verb:
    $Common.PronounMe? $Common.Lets
    %lemma
    включай
    включи
    воспроизведи
    давай все таки
    давай послушаем
    давай расскажи
    давай поставь
    давай прочитай
    есть еще
    запусти
    запустить
    зачитай
    можешь сказать
    можешь показать
    найди
    не подскажешь
    открой
    поведай
    поищи
    покажи
    послушаем
    послушать
    поставь
    поставить
    почитай
    прочитай
    прочитать
    прочти
    расскажи
    расскажите
    рассказать
    рассказывай
    скажи
    слушать
    узнай

$MoreVerb:
    %lemma
    больше
    другие
    других
    новые
    новых
    еще

$More:
    [есть? $MoreVerb]
    дальше
    продолжи
    продолжить
    пропусти
    пропустить
    %lemma
    другие
    другой
    новые
    новых
    следующая
    следующие
    следующий
    следующую

$News:
    %lemma
    происшествия
    новость
    новости
    новостей
    топ? новостей
    выпуск новостей
    событии
    сюжет
    сюжетов
    сюжеты
