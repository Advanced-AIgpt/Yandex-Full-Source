import: voice/common.grnt

form alice.voice.whisper.what_is_it:
    root:
        [$Explain+ ($Setting | $Speak)* $Whisper+]

    filler:
        $Filler
        $ExplanationProperty
        $SettingPtoperty

$ExplanationProperty:
    %lemma
    %enable_synonyms all

    вкратце
    ли
    не
    правильно
    подробнее

$SettingPtoperty:
    %lemma
    %enable_synonyms all

    новинка
    новый
    последний
    такой
    функция
