import: common/commands.grnt
import: common/filler.grnt
import: common/pronouns.grnt
import: common/surfaces.grnt

form alice.onboarding.starting_configure_success:
    root:
        [$Common.Lets* $Common.PronounMe* $Action* $Common.Open* $First* $Greeting+ $About* $Common.SmartSpeaker+]
    filler:
        $nonsense
        $Common.Filler
        $Common.WayToCallAlice

$Action:
    %lemma
    прочитать
    сказать
    рассказать
    повторить

$First:
    %lemma
    первый
    стартовый
    начальный

$Greeting:
    приветствие
    знакомство

$About:
    про
    о
    об
    с
    со
