import: alice_show/common.grnt

form alice.alice_show.good_night:
    root:
        [$GoodNight+ я* $Goto* $Sleep*]
        [я* $Goto+ $Sleep+]
        [я+ $Sleep+]

    filler:
        $Filler
        $ForGreetings
        все

$Goto:
    иду
    идти
    идем
    лечу
    ложиться
    ложусь
    ложимся
    давай
    пойдем
    пойду

$Sleep:
    спать
    сплю
    спим
    засыпать
    засыпаю
    засыпаем
    уснуть
    уснул
    уснули
