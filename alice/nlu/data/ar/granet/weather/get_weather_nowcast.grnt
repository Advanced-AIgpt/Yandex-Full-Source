form personal_assistant.scenarios.get_weather_nowcast:
    enable_alice_tagger: true
    enable_granet_parser: false
    slots:
        prec_type_asked:
            type:
                custom.prec_type
        expected_change:
            type:
                custom.expected_change
        when:
            type:
                sys.datetime_range
                sys.datetime
        day_part:
            type:
                custom.day_part
        where:
            type: string
