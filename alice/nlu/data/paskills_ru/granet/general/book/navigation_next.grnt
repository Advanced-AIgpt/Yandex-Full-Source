import: common/filler.grnt
import: general/book/common.grnt

form YANDEX.BOOK.NAVIGATION.NEXT:
    root:
        [$Next $More? $AudioBook?]
        [$Skip $More? $AudioBook?]
        [$Skip (дальше|вниз|вправо)]
        $Phrase
    filler:
        $Common.Filler
        $Common.WayToCallAlice
        $Common.LeftFiller

$Skip:
    %lemma
    скип
    skip
    пропусти
    переключи
    промотай
    перелистни
    листай
    пролистай
    скипни
    скипай
    поменяй
    мотай

$More:
    еще

$Next:
    %lemma
    другой
    следующий

    %lemma off
    дальше
    вперед
    вниз
    вправо

$Phrase:
    %fillers off
    давай еще
    и еще
    посмотрим что дальше
    дальше говорю
    крути дальше
