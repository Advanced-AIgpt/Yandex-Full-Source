{
    "dataset": {
        "source": "nlu",
        "intent": [
            "personal_assistant.scenarios.music_play"
        ],
        "negatives_to_remove": [
            "music_general",
            "alarm_ask_sound",
            "music_play_anaphora",
            "music_play_more",
            "music_ambient_sound",
            "player_continue",
            "music_what_is_playing__play"
        ],
        "filter_full_textual_intersections": true,
        "validation_split": 0.1
    },
    "model": {
        "embedding": "context_embedding",
        "input_vector_size": 300,
        "hidden_layer_sizes": [300, 300],
        "batch_size": 512,
        "epoch_count": 10
    }
}