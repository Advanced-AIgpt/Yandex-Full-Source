{
    "build": {
        "targets": [
            "alice/bass",
            "alice/bass/tools/yav_wrapper",
            "alice/rtlog/evlogdump"
        ]
    },
    "data": [
        {
            "destination": {
                "path": "/bin/"
            },
            "source": {
                "files": [
                    "evlogdump"
                ],
                "path": "alice/rtlog/evlogdump",
                "type": "BUILD_OUTPUT"
            }
        },
        {
            "destination": {
                "path": "/bin/"
            },
            "source": {
                "files": [
                    "bass_server"
                ],
                "path": "alice/bass/bin",
                "type": "BUILD_OUTPUT"
            }
        },
        {
            "destination": {
                "path": "/bass_configs/"
            },
            "source": {
                "files": [
                    "*.json"
                ],
                "path": "alice/bass/configs",
                "type": "ARCADIA"
            }
        },
        {
            "destination": {
                "path": "/bass_scripts/"
            },
            "source": {
                "files": [
                    "push_client_log_parsing.py"
                ],
                "path": "alice/bass/scripts",
                "type": "ARCADIA"
            }
        },
        {
            "destination": {
                "path": "/"
            },
            "source": {
                "id": 3069941764,
                "path": "geodata6.bin",
                "type": "SANDBOX_RESOURCE"
            }
        },
        {
            "destination": {
                "path": "/bin/"
            },
            "source": {
                "files": [
                    "yav_wrapper"
                ],
                "path": "alice/bass/tools/yav_wrapper",
                "type": "BUILD_OUTPUT"
            }
        }
    ],
    "meta": {
        "description": "BASS package.",
        "maintainer": "Ruslan Akhtariev <akhruslan@yandex-team.ru>",
        "name": "bass_package",
        "version": "{revision}.{branch}.{sandbox_task_id}"
    }
}
