{% phrase cond %}
    Ladies and gentlemen,
    {% if context.a in ('John', 'Paul', 'George', 'Ringo') %}
        The Beatles!
    {% elif context.a == 'Elton' %}
        The bitch is back!
    {% elif context.a == 'Ozzy' %}
        Prince of darkness!
    {% else %}
        The misterious stranger!
    {% endif %}
{% endphrase %}
