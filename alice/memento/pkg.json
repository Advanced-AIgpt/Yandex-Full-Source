{
    "meta": {
        "name": "memento",
        "maintainer": "PASkills",
        "description": "Alice memento",
        "version": "{branch}.{svn_revision}.{revision}"
    },
    "build": {
        "targets": [
            "alice/memento"
        ],
        "target-platforms": [
            "DEFAULT-LINUX-X86_64"
        ]
    },
    "data": [
        {
            "source": {
                "type": "RELATIVE",
                "path": "Dockerfile"
            },
            "destination": {
                "path": "/Dockerfile"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": ".dockerignore"
            },
            "destination": {
                "path": "/.dockerignore"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "misc",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/misc/"
            }
        },
        {
            "source": {
                "type": "RELATIVE",
                "path": "config",
                "files": [
                    "*"
                ]
            },
            "destination": {
                "path": "/config/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "alice/memento/memento",
                "exclude": [
                    "alice-protos-data.jar",
                    "api-grpc-draft.jar",
                    "api-grpc.jar",
                    "api-protos.jar",
                    "library-censor-protos.jar",
                    "library-java-svnversion.jar",
                    "library-proto_eval-proto.jar",
                    "library-restriction_level-protos.jar",
                    "megamind-protos-common.jar",
                    "megamind-protos-proactivity.jar",
                    "memento-scanner.jar",
                    "memento.jar",
                    "memento_proto.jar",
                    "metrics-jvm.jar",
                    "metrics.jar",
                    "paskills-ydb-client.jar",
                    "plugins-grpc_input-proto.jar",
                    "profile-proto-context.jar",
                    "proto-data-analytics.jar",
                    "protos-data-search_result.jar",
                    "protos-data-video.jar",
                    "solomon-utils.jar",
                    "tvm-spring-handler.jar",
                    "tvm_with_metrics.jar",
                    "tvmauth-java.jar",
                    "unified_agent_java_client.jar",
                    "unified_agent_java_client_log4j2.jar",
                    "ydb-core.jar",
                    "ydb-table.jar",
                    "yt-interface-protos.jar"
                ]
            },
            "destination": {
                "path": "/memento/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "alice/memento/memento",
                "files": [
                    "alice-protos-data.jar",
                    "api-grpc-draft.jar",
                    "api-grpc.jar",
                    "api-protos.jar",
                    "library-censor-protos.jar",
                    "library-java-svnversion.jar",
                    "library-proto_eval-proto.jar",
                    "library-restriction_level-protos.jar",
                    "megamind-protos-common.jar",
                    "megamind-protos-proactivity.jar",
                    "memento-scanner.jar",
                    "memento_proto.jar",
                    "metrics-jvm.jar",
                    "metrics.jar",
                    "paskills-ydb-client.jar",
                    "plugins-grpc_input-proto.jar",
                    "profile-proto-context.jar",
                    "proto-data-analytics.jar",
                    "protos-data-search_result.jar",
                    "protos-data-video.jar",
                    "solomon-utils.jar",
                    "tvm-spring-handler.jar",
                    "tvm_with_metrics.jar",
                    "tvmauth-java.jar",
                    "unified_agent_java_client.jar",
                    "unified_agent_java_client_log4j2.jar",
                    "ydb-core.jar",
                    "ydb-table.jar",
                    "yt-interface-protos.jar"
                ]
            },
            "destination": {
                "path": "/arcadia_libs/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "alice/memento/memento.jar"
            },
            "destination": {
                "path": "/"
            }
        },
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "{package_root}/jdk"
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
